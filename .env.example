# Ollama (lokal, kostenlos)
OLLAMA_HOST=http://host.docker.internal:11434
OLLAMA_MODEL=qwen2.5:7b

# Betrieb
AUTONOMY_MODE=full
GOAL_BATCH_SIZE=3
CYCLE_PAUSE_SECONDS=60
MAX_ITERATIONS=30
EVOLUTION_SNAPSHOT_HOURS=24
WEB_INTERFACE=true

# Google Gemini Key (https://aistudio.google.com)
GOOGLE_API_KEY=DEIN_GEMINI_KEY_HIER

# Optional
ANTHROPIC_API_KEY=optional
OPENAI_API_KEY=optional

# Telegram Bot (https://t.me/BotFather)
TELEGRAM_BOT_TOKEN=DEIN_TELEGRAM_TOKEN
TELEGRAM_ALLOWED_USERS=DEINE_TELEGRAM_USER_ID
